{
    "multipart": [
        {   "apply": { "model": "lepton:block/paper_wall_sakura_post" }},
        {   "when": { "north": true },
            "apply": { "model": "lepton:block/paper_wall_sakura_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "lepton:block/paper_wall_sakura_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "lepton:block/paper_wall_sakura_side_alt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "lepton:block/paper_wall_sakura_side_alt", "y": 90 }
        },
        {   "when": { "north": false },
            "apply": { "model": "lepton:block/paper_wall_sakura_noside" }
        },
        {   "when": { "east": false },
            "apply": { "model": "lepton:block/paper_wall_sakura_noside_alt" }
        },
        {   "when": { "south": false },
            "apply": { "model": "lepton:block/paper_wall_sakura_noside_alt", "y": 90 }
        },
        {   "when": { "west": false },
            "apply": { "model": "lepton:block/paper_wall_sakura_noside", "y": 270 }
        }
    ]
}
